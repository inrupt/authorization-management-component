{
  "name": "authorization-management-component",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "build": "next build",
    "prepare:e2e": "cd e2e/test-app ; npm ci; npm run build",
    "start:e2e": "cd e2e/test-app ; npm run start",
    "dev:e2e": "cd e2e/test-app ; npm ci ; npm run dev",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint": "npm run lint:eslint; npm run lint:prettier",
    "lint:fix": "npm run lint:eslint -- --fix; npm run lint:prettier -- --write",
    "lint:eslint": "eslint . --ext=.js,.jsx,.ts,.tsx",
    "lint:prettier": "prettier --check .",
    "prepare": "husky install && npm run preparePurposes",
    "start": "next start",
    "test": "TZ=UTC jest --runInBand",
    "test:e2e": "playwright test",
    "update-clientId": "npm ci && npx ts-node --skip-project scripts/updateClientId.ts",
    "bump-version": "changeset add",
    "version": "changeset version",
    "release": "changeset publish",
    "preparePurposes": "npx ts-node --skip-project scripts/preparePurposes.ts"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@comunica/context-entries": "^2.8.1",
    "@comunica/core": "^2.8.1",
    "@inrupt/solid-client": "^1.23.3",
    "@inrupt/solid-client-access-grants": "^3.0.4",
    "@inrupt/solid-client-authn-browser": "^2.0.0",
    "@inrupt/solid-client-authn-node": "^2.0.0",
    "@inrupt/solid-client-vc": "^1.0.0",
    "@popperjs/core": "^2.11.8",
    "@rdfjs/types": "^1.1.0",
    "@sentry/nextjs": "^7.77.0",
    "@types/accepts": "^1.3.5",
    "@types/n3": "^1.16.0",
    "accepts": "^1.3.8",
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.9.1",
    "event-emitter-promisify": "^1.1.0",
    "n3": "^1.17.0",
    "next": "^14.1.1",
    "rdf-dereference": "^2.1.0",
    "rdf-namespaces": "^1.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "reactstrap": "^9.2.0",
    "sass": "^1.55.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.7.3",
    "@inrupt/eslint-config-base": "^3.2.1",
    "@inrupt/eslint-config-react": "^3.2.1",
    "@inrupt/internal-playwright-helpers": "^3.0.2",
    "@inrupt/internal-playwright-testids": "^3.1.0",
    "@inrupt/internal-test-env": "^3.0.2",
    "@inrupt/jest-jsdom-polyfills": "^3.2.1",
    "@next/eslint-plugin-next": "^12.2.4",
    "@playwright/test": "~1.37.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/axe-core": "^2.0.7",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.6.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "axe-core": "^4.7.2",
    "dotenv-flow": "^3.2.0",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-header": "^3.1.1",
    "husky": "^9.0.11",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --ext=.js,.jsx,.ts,.tsx --fix --max-warnings=0",
    "*.{js,jsx,ts,tsx,css,md,html,json}": "prettier --write"
  }
}
