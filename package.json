{
  "name": "authorization-management-component",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "build": "next build",
    "prepare:e2e": "cd e2e/test-app ; npm ci; npm run build",
    "start:e2e": "cd e2e/test-app ; npm run start",
    "dev:e2e": "cd e2e/test-app ; npm ci ; npm run dev",
    "dev": "next dev",
    "format": "prettier --write .",
    "lint": "npm run lint:eslint",
    "lint:fix": "npm run lint:eslint -- --fix",
    "lint:eslint": "eslint --config eslint.config.mjs .",
    "prepare": "husky && npm run preparePurposes",
    "start": "next start",
    "test": "TZ=UTC jest --runInBand",
    "test:e2e": "playwright test",
    "update-clientId": "npm ci && tsc --project tsconfig.scripts.json && node dist/scripts/updateClientId.js",
    "bump-version": "changeset add",
    "version": "changeset version",
    "release": "changeset publish",
    "preparePurposes": "tsc --project tsconfig.scripts.json && node dist/scripts/preparePurposes.js ./src/cache"
  },
  "dependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.4",
    "@comunica/context-entries": "^4.1.0",
    "@comunica/core": "^4.0.2",
    "@inrupt/solid-client": "^2.1.2",
    "@inrupt/solid-client-access-grants": "^3.3.1",
    "@inrupt/solid-client-authn-browser": "^2.5.0",
    "@inrupt/solid-client-authn-node": "^3.1.0",
    "@inrupt/solid-client-vc": "^1.1.0",
    "@popperjs/core": "^2.11.8",
    "@rdfjs/types": "^2.0.1",
    "@sentry/nextjs": "^9.28.0",
    "@types/accepts": "^1.3.7",
    "@types/n3": "^1.26.0",
    "accepts": "^1.3.8",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "event-emitter-promisify": "^1.1.0",
    "n3": "^1.26.0",
    "next": "^15.3.4",
    "rdf-dereference": "^4.0.0",
    "rdf-namespaces": "^1.13.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "reactstrap": "^9.2.3",
    "sass": "^1.89.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@inrupt/eslint-config-base": "^4.0.3",
    "@inrupt/internal-playwright-helpers": "^4.0.3",
    "@inrupt/internal-playwright-testids": "^4.0.3",
    "@inrupt/internal-test-env": "^4.0.3",
    "@inrupt/jest-jsdom-polyfills": "^4.0.3",
    "@next/eslint-plugin-next": "^15.2.2",
    "@playwright/test": "~1.53.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/axe-core": "^3.0.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "axe-core": "^4.10.3",
    "dotenv-flow": "^4.1.0",
    "eslint": "^9.30.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.2",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --ext=.js,.jsx,.ts,.tsx --fix --max-warnings=0",
    "*.{js,jsx,ts,tsx,css,md,html,json}": "prettier --write"
  }
}
